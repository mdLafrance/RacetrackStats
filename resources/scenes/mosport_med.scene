# Medium quality mosport track

# Meshes
mesh jr_mosport.obj
mesh mosport_horizon.obj
mesh mosport_vehicles.obj
mesh mosport_objects.obj
mesh mosport_layers.obj
mesh mosport_trees.obj
mesh mosport_people.obj
mesh mosport_crew.obj

mesh BMW.obj

# Materials
material jr_mosport.mtl
material mosport_horizon.mtl
material mosport_vehicles.mtl
material mosport_objects.mtl
material -t mosport_layers.mtl
material -t mosport_trees.mtl
material mosport_people.mtl
material mosport_crew.mtl

material -t BMW.mtl

# Scene ambient light (r g b)
ambient 0.3 0.3 0.27

# Lights for the scene
light directional -i 2 -v -2 -10 -5 -c 0.65 0.6 0.6 
